@model IEnumerable<MVCGrid.Models.Personel>
@{
    ViewBag.Title = "Listele";
    Layout = "~/Views/Shared/_LayoutPage.cshtml";
}

<h2>Personelleri Listele</h2>

<ul class="list-group">
    @foreach (var Item in Model)
    {
        <li class="list-group-item">
            @Item.Ad
        </li>
    }
</ul>

